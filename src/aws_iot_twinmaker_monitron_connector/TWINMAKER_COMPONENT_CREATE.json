        },
        "isInherited": false
      },
      "schemaInitializer": {
        "implementedBy": {
          "lambda": {
            "arn": "arn:aws:lambda:<YOUR_REGION>:<YOUR_ACCOUNT_NUMBER>:function:TWINMAKER_SCHEMA_INITIALIZATION"
          },
          "isNative": false
        },
        "isInherited": false
      }
    }
  }