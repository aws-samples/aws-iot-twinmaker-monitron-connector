    "functions": {
      "dataReader": {
        "implementedBy": {
          "lambda": {
            "arn": "arn:aws:lambda:<YOUR_REGION>:<YOUR_ACCOUNT_NUMBER>:function:TWINMAKER_DATA_READER"
          },
